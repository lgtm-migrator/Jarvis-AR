{
  "name": "my-new-project",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "REACT_DEBUGGER='open -g rndebugger://set-debugger-loc?port=8081 ||' node node_modules/react-native/local-cli/cli.js start",
    "preandroid": "npm run predeploy:android",
    "android": "react-native run-android",
    "preios": "node dev/ios",
    "ios": "react-native run-ios",
    "test": "jest",
    "prettier": "prettier './src/**/*.{js,scss}' --write",
    "flow": "flow",
    "glow": "glow",
    "glow:watch": "glow --watch",
    "deploy:android": "cd android && fastlane playstore",
    "predeploy:android": "node dev/fastlane",
    "postdeploy:android": "node dev/fastlane/post",
    "predeploy:ios": "npm run preios",
    "deploy:ios": "cd ios && fastlane beta"
  },
  "dependencies": {
    "apsl-react-native-button": "3.1.1",
    "array-flat-polyfill": "1.0.1",
    "react": "16.6.3",
    "react-native": "0.58.6",
    "react-native-auth0": "1.6.0",
    "react-native-camera": "1.11.2",
    "react-native-config": "0.11.7",
    "react-native-firebase-mlkit": "0.6.2",
    "react-native-gesture-handler": "1.0.15",
    "react-native-vector-icons": "6.3.0",
    "react-navigation": "3.3.0",
    "react-redux": "6.0.0",
    "redux": "4.0.1",
    "redux-persist": "5.10.0",
    "redux-saga": "1.1.1",
    "reselect": "4.0.0"
  },
  "devDependencies": {
    "@babel/runtime": "7.6.2",
    "babel-core": "6.26.3",
    "dotenv": "8.1.0",
    "flow-bin": "0.109.0",
    "glow": "1.2.2",
    "jest": "24.9.0",
    "metro-react-native-babel-preset": "0.56.0",
    "prettier": "1.18.2",
    "react-test-renderer": "16.6.3"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!native-base?!(jest-))/"
    ],
    "globals": {
      "window": {}
    }
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts"
    ]
  }
}
